{#
/**
 * @file
 * Implementation of a panels grid ready layout.
 *
 * Available variables:
 * - $content: An array of content, each item in the array is keyed to one
 *   panel of the layout.
 * - $css_id: unique id if present.
 */
#}
<div class="panel-display parrot-panels three-25-50-25 clearfix"
{% if css_id %}
  id="{{ css_id }}"
{% endif %}>
{% if content.three_25_50_25_top %}
  <div id="three-25-50-25-top-wrapper" class="fullwidth">
    <div class="container">
      <div class="region three-25-50-25-top region-conditional-stack">
        <div class="region-inner clearfix">
          {{ content.three_25_50_25_top }}
        </div>
      </div>
    </div>
  </div>
{% endif %}
<div id="three-25-50-25-middle-wrapper" class="fullwidth">
  <div class="container">
  {% if content.three_25_50_25_first %}
  <div class="region three-25-50-25-first">
      <div class="region-inner clearfix">
        {{ content.three_25_50_25_first }}
      </div>
    </div>
  {% endif %}
  {% if content.three_25_50_25_second %}
    <div class="region three-25-50-25-second">
      <div class="region-inner clearfix">
        {{ content.three_25_50_25_second }}
      </div>
    </div>
  {% endif %}
  {% if content.three_25_50_25_third %}
    <div class="region three-25-50-25-third">
      <div class="region-inner clearfix">
        {{ content.three_25_50_25_third }}
      </div>
    </div>
  {% endif %}
  </div>
</div>
{% content.three_25_50_25_bottom %}
  <div id="three-25-50-25-bottom-wrapper" class="fullwidth">
    <div class="container">
      <div class="region three-25-50-25-bottom region-conditional-stack">
        <div class="region-inner clearfix">
          {{ content.three_25_50_25_bottom }}
        </div>
      </div>
    </div>
  </div>
{% endif %}
</div>
